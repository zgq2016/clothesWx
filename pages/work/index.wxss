.main {
  width: 100vw;
  display: flex;
  justify-content: center;
  align-items: center;
}
.main .search_input {
  width: 700rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.main .search_input input {
  position: relative;
  padding: 0 40rpx;
  width: 710rpx;
  height: 70rpx;
  background-color: #fff;
  border-radius: 100rpx;
}
.main .search_input input .icon-small {
  position: absolute;
  right: 40rpx;
  top: 50%;
  transform: translate(0, -50%);
}
.main .search_input .bianji {
  width: 60rpx;
  height: 60rpx;
  background-color: #fff;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 20rpx;
}
.main .search_input .bianji image {
  width: 50rpx;
  height: 50rpx;
}
.style_list {
  margin: 20rpx 35rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
}
.style_list .list {
  width: 328rpx;
  height: 684rpx;
  margin: 0 0 60rpx 0;
}
.style_list .list .img {
  text-align: center;
}
.style_list .list .img image {
  width: 320rpx;
  height: 600rpx;
  border-radius: 20rpx;
}
.style_list .list .con {
  font-style: PingFang SC;
  padding: 0 20rpx;
}
.style_list .list .con .name {
  font-size: 30rpx;
  font-weight: 900;
  margin: 10rpx 0;
}
.style_list .list .con .username {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  color: #c6c6c6;
}
.card_list {
  width: 100vw;
  background-color: #fff;
}
.card_list .list {
  display: flex;
  padding: 20rpx 10rpx;
  width: 730rpx;
}
.card_list .list .listLeft {
  width: 730rpx;
  height: 120rpx;
  display: flex;
}
.card_list .list .listLeft .listLeftImg {
  width: 280rpx;
  height: 120rpx;
  display: flex;
}
.card_list .list .listLeft .listLeftImg .image1 {
  position: relative;
  width: 120rpx;
  height: 120rpx;
  margin: 0 10rpx;
  overflow: hidden;
  border-radius: 10rpx;
}
.card_list .list .listLeft .listLeftImg .image1 image {
  width: 120rpx;
  position: absolute;
  top: 50%;
  left: 0;
  transform: translate(0, -50%);
}
.card_list .list .listLeft .listLeftImg .image2 {
  position: relative;
  width: 120rpx;
  height: 120rpx;
  margin: 0 10rpx;
  overflow: hidden;
  border-radius: 10rpx;
}
.card_list .list .listLeft .listLeftImg .image2 image {
  width: 120rpx;
  position: absolute;
  top: 50%;
  left: 0;
  transform: translate(0, -50%);
}
.card_list .list .listLeft .listLeftInfo {
  flex: 1;
  padding: 10rpx 20rpx;
  border-bottom: 0.01rem solid #efefef;
}
.card_list .list .listLeft .listLeftInfo .listLeftInfoName {
  font-size: 26rpx;
  font-weight: 600;
  color: #000;
}
.card_list .list .listLeft .listLeftInfo view {
  color: #8f8f8f;
}
.newProject {
  width: 100rpx;
  height: 100rpx;
  background-color: #333;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  right: 30rpx;
  bottom: 110rpx;
  z-index: 100;
}
.newProject .iconfont {
  font-size: 50rpx;
  font-weight: 600;
  color: #fff;
}
.newProject .newProjectShow {
  width: 100rpx;
  height: 300rpx;
  background-color: #333;
  position: fixed;
  right: 30rpx;
  bottom: 110rpx;
  border-radius: 100rpx;
}
.newProject .newProjectShow navigator {
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
page {
  margin-bottom: 200rpx;
}
