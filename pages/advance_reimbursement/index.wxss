.header {
  width: 100vw;
  height: 70rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 20rpx;
}
.header .header_info view {
  margin: 0 20rpx;
}
.header .operation {
  display: flex;
  align-items: center;
}
.header .operation view {
  background-color: #000;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  margin: 0 20rpx;
}
.header .operation view image {
  width: 40rpx;
  height: 40rpx;
}
page {
  position: relative;
}
.search_form {
  position: absolute;
  top: 0;
  right: 0;
  width: 100vw;
  height: 100vh;
  background-color: #fff;
  z-index: 100;
}
.search_form .form {
  margin: 10rpx 40rpx;
  width: 670rpx;
  font-size: 30rpx;
}
.search_form .form .icon-icon-test3 {
  display: flex;
  justify-content: center;
  align-items: center;
  -webkit-transform: rotate(270deg);
  transform: rotate(270deg);
  color: #000;
}
.search_form .form .picker {
  width: 670rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.search_form .form .picker .name {
  width: 130rpx;
  margin-right: 30rpx;
}
.search_form .form .picker .con {
  display: flex;
  align-items: center;
}
.search_form .form .picker .con .placeholder {
  margin-right: 10rpx;
  color: #ccc;
}
.search_form .form .date {
  border-bottom: 1rpx solid #f2f2f2;
  height: 100rpx;
}
.search_form .next_step {
  width: 600rpx;
  height: 80rpx;
  background-color: #000;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 200rpx;
  position: fixed;
  bottom: 30rpx;
  left: 50%;
  transform: translate(-50%, 0);
}
.right_form {
  position: absolute;
  top: 100rpx;
  right: 10rpx;
  width: 150rpx;
  height: 0;
  background-color: #4c4c4c;
  opacity: 0;
  border-radius: 10rpx;
}
.right_form .arrows {
  position: absolute;
  top: -10rpx;
  right: 30rpx;
  width: 10px;
  height: 10px;
  border-width: 10px;
  border-color: #4c4c4c;
  border-top-width: 0;
  border-style: dashed;
  border-bottom-style: solid;
  border-left-color: transparent;
  border-right-color: transparent;
}
.right_form .go_select {
  width: 150rpx;
  height: 300rpx;
}
.right_form .go_select navigator {
  width: 150rpx;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
}
.newProject {
  width: 100rpx;
  height: 100rpx;
  background-color: #000;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  right: 30rpx;
  bottom: 110rpx;
  color: #fff;
}
.newProject .iconfont {
  font-size: 50rpx;
  font-weight: 600;
}
.newProject .newProjectShow {
  width: 100rpx;
  height: 300rpx;
  background-color: #000;
  position: fixed;
  right: 30rpx;
  bottom: 110rpx;
  border-radius: 20rpx;
}
.newProject .newProjectShow navigator {
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.newProject .newProjectShow .iconfont {
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 50rpx;
  font-weight: 600;
}
