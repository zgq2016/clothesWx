.projectDetails .userInfo {
  height: 460rpx;
  background-color: #535353;
  padding: 30rpx 30rpx;
}
.projectDetails .userInfo .info {
  height: 280rpx;
  display: flex;
  color: #fff;
}
.projectDetails .userInfo .info .image1 {
  position: relative;
  width: 260rpx;
  height: 260rpx;
  margin: 0 10rpx;
  overflow: hidden;
  border-radius: 10rpx;
}
.projectDetails .userInfo .info .image1 image {
  height: 260rpx;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translate(-50%, 0);
}
.projectDetails .userInfo .info .info_img image {
  width: 260rpx;
  height: 260rpx;
  border-radius: 20rpx;
}
.projectDetails .userInfo .info .info_con {
  margin: 10rpx 0 0 30rpx;
}
.projectDetails .userInfo .info .info_con .projectname {
  font-size: 32rpx;
}
.projectDetails .userInfo .info .info_con view {
  padding: 4rpx 0;
  font-size: 28rpx;
}
.projectDetails .userInfo .info .info_con .require {
  width: 360rpx;
  display: -webkit-box;
  overflow: hidden;
  white-space: normal !important;
  text-overflow: ellipsis;
  word-wrap: break-word;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.projectDetails .userInfo .operate {
  display: flex;
}
.projectDetails .userInfo .operate .bianji {
  width: 60rpx;
  height: 60rpx;
  background-color: #f2f2f2;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 20rpx;
}
.projectDetails .userInfo .operate .bianji image {
  width: 50rpx;
  height: 50rpx;
}
.style_list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
  margin: 20rpx 35rpx;
}
.style_list .list {
  width: 328rpx;
  height: 684rpx;
  margin: 0 0 60rpx 0;
}
.style_list .list .img {
  text-align: center;
}
.style_list .list .img image {
  width: 320rpx;
  height: 600rpx;
  border-radius: 20rpx;
}
.style_list .list .con {
  font-style: PingFang SC;
  padding: 0 20rpx;
}
.style_list .list .con .name {
  font-size: 30rpx;
  font-weight: 900;
  margin: 10rpx 0;
}
.style_list .list .con .username {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  color: #c6c6c6;
}
.projectList {
  width: 100%;
  background-color: #fff;
}
.projectList .list {
  display: flex;
  padding: 20rpx 10rpx;
  border-bottom: 1px solid #ccc;
}
.projectList .list .listLeft {
  flex: 1;
  display: flex;
}
.projectList .list .listLeft .listLeftImg {
  height: 120rpx;
  display: flex;
}
.projectList .list .listLeft .listLeftImg .image1 {
  position: relative;
  width: 120rpx;
  height: 120rpx;
  margin: 0 10rpx;
  overflow: hidden;
  border-radius: 10rpx;
}
.projectList .list .listLeft .listLeftImg .image1 image {
  width: 120rpx;
  position: absolute;
  top: 50%;
  left: 0;
  transform: translate(0, -50%);
}
.projectList .list .listLeft .listLeftImg .image2 {
  position: relative;
  width: 120rpx;
  height: 120rpx;
  margin: 0 10rpx;
  overflow: hidden;
  border-radius: 10rpx;
}
.projectList .list .listLeft .listLeftImg .image2 image {
  width: 120rpx;
  position: absolute;
  top: 50%;
  left: 0;
  transform: translate(0, -50%);
}
.projectList .list .listLeft .listLeftInfo {
  padding: 10rpx 20rpx;
}
.projectList .list .listLeft .listLeftInfo .listLeftInfoName {
  font-size: 26rpx;
  font-weight: 600;
  color: #000;
}
.projectList .list .listLeft .listLeftInfo view {
  color: #8f8f8f;
}
.projectList .list .listright {
  width: 150rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.newProject {
  width: 100rpx;
  height: 100rpx;
  background-color: #000;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  right: 30rpx;
  bottom: 110rpx;
  color: #fff;
}
.newProject .iconfont {
  font-size: 50rpx;
  font-weight: 600;
}
.newProject .newProjectShow {
  width: 100rpx;
  height: 200rpx;
  background-color: #000;
  position: fixed;
  right: 30rpx;
  bottom: 110rpx;
  border-radius: 100rpx;
}
.newProject .newProjectShow view {
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 20rpx;
}
page {
  margin-bottom: 200rpx;
}
