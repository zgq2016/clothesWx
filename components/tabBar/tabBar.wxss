.tabBar {
  display: flex;
  justify-content: space-around;
  align-items: center;
  width: 100vw;
  height: 100rpx;
  background-color: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  color: #000;
  border-top: 0.01rem solid #f1f1f1;
  z-index: 999;
}
.tabBar .index {
  flex: 1;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
